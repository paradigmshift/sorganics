<%= form_for @inventory do |f| %>
<div class='form-group'>
  <%= f.label :date %>
  <%= f.text_field :date, "data-provide" => 'datepicker' %>
</div>
<div class='form-group'>
  <%= f.label :quantity %>
  <%= f.number_field :quantity %>
</div>
<div class='form-group'>
  <%= f.label :variety %>
  <%= f.collection_select :feed_type_id, FeedType.all, :id, :variety %>
</div>
<%= f.submit (@inventory.new_record? ? "Add Inventory" : "Update Inventory") %>
<% end %>
